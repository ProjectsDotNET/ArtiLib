@model ArtiLib.Entites.RendezVous

@{
    ViewBag.Title = "ArtiLib - Supprimer le rendez-vous";
}

<h2>Supprimer le rendez-vous</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.IdClient)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IdClient)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.IdDisponibiliteArtisan)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IdDisponibiliteArtisan)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.IdStatutRendezVous)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IdStatutRendezVous)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.IdRendezVousDeplace)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IdRendezVousDeplace)
        </dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Supprimer" class="btn btn-default" /> |
            @Html.ActionLink("Retour à la liste des rendez-vous", "Index")
        </div>
    }
</div>